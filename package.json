{
  "name": "tdw-gustavo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env HIDE_BABELRC=true npm test && next dev && node -e \"require('fs').renameSync('.babelrc_hidden', '.babelrc');\"",
    "build": "node -e \"require('fs').renameSync('.babelrc', '.babelrc_hidden');\" && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "node -e \"if (!require('fs').existsSync('.babelrc') && require('fs').existsSync('.babelrc_hidden')) { require('fs').renameSync('.babelrc_hidden', '.babelrc'); }\" && jest && node -e \"if (require('fs').existsSync('.babelrc')) { require('fs').renameSync('.babelrc', '.babelrc_hidden'); }\"",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@types/node": "20.6.3",
    "@types/react": "18.2.22",
    "@types/react-dom": "18.2.7",
    "eslint": "8.49.0",

    "next": "^13.5.4",
    "node-sass": "^9.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.68.0",
    "typescript": "5.2.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.0",
    "@jest/globals": "^29.7.0",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.5",
    "@types/testing-library__jest-dom": "^6.0.0",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "3.0.3",
    "ts-jest": "^29.1.1"
  }
}
